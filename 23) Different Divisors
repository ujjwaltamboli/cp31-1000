#include <bits/stdc++.h>
using namespace std;
bool isPrime(long long n) {
    if (n < 2) return false;
    for (long long i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) return false;
    }
    return true;
} //sqrt(n);

int next_prime(long long y) {
    while (true) {//loge(d)=10
        if (isPrime(y)) return y;//sqrt(pow(10,4))=pow(10,2)=100
        y++;
    }
}//loge(n)*sqrt(n)=== loge(10000)*sqrt(10000)=pow(10,3)=1000
int main(){
    int t;
    cin>>t;
    while (t--){
        int d;
        cin>>d;
        int p=next_prime(d+1);
        int q=next_prime(d+p);
        int mini= min(1LL*p*p*p,1LL*p*q);
        cout<<mini<<endl;
    }

    //T.C.:- O(pow(10,3));
    //s.c:- O(1)

}
