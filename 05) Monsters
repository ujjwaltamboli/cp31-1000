#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n, k;
        cin>>n>>k;
        vector<vector<int>> store(n,vector<int>(2,0));
        for (int i=0;i<n;i++){
            cin>>store[i][0];
            store[i][0]=store[i][0]%k;
            if (store[i][0]==0){
                store[i][0]=INT_MAX;
            }
            store[i][1]=i+1;
        }
        stable_sort(store.begin(), store.end(), [](const vector<int> &a, const vector<int> &b){
            return a[0]>b[0];
        });
        for (int i=0;i<n;i++){
            cout<<store[i][1]<<" ";
        }
        cout<<endl;

        //T.C.:- O(nlogn)
        //S.C.:- O(n*2)
    }
    return 0;
}
