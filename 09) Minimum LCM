#include <bits/stdc++.h>
using namespace std;

int findFactor(int n) {
    for (int i = 2; i*i <= n; ++i) {
        if (n % i == 0) {
            return n/i;
        }
    }
    return n/n;
}

int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int a=findFactor(n);
        cout<<a<<" "<<(n-a)<<endl;
    }

    //T.C.:- O(sqrt(n));
    //S.C.:- O(1);
    return 0;
}

