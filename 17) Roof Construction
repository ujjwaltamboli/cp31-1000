#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int MSB=log2(n-1);
        int ans=pow(2,MSB);
        for (int i=ans-1;i>=0;i--){
            cout<<i<<" ";
        }
        for (int i=ans;i<n;i++){
            cout<<i<<" ";
        }
        cout<<endl;
    }

    //T.c.:- O(n)
    //S.C.:- O(n)
    return 0;
}
