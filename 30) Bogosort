#include <bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        vector<int> store(n,0);
        for (int i=0;i<n;i++){
            cin>>store[i];
        }
        sort(store.rbegin(),store.rend());
        for (int i=0;i<n;i++){
            cout<<store[i]<<" ";
        }
        cout<<endl;
    }
    //T.C.:- O(nlogn)
    //S.C.:- O(n)
    return 0;
}
