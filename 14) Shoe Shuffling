#include <bits/stdc++.h>
using namespace std;
int main(){
    long long t;
    cin>>t;
    while (t--){
        long long n;
        cin>>n;
        vector<long long> store(n,0);
        map<long long,long long> freq;
        bool flag=true;
        for (long long i=0;i<n;i++){
            cin>>store[i];
            freq[store[i]]++;
        }

        for (auto it=freq.begin(); it!=freq.end();it++){
            if (it->second<=1){
                flag=false;
                break;
            }
        }
        if (!flag){
            cout<<-1<<endl;
            continue;
        }
        long long val=store[0];
        long long ind=0;
        vector<long long> ans(n,0);
        for (long long i=1;i<n;i++){
            if (store[i]==val){
                ans[i-1]=i+1;
                ans[i]=ind+1;
            }
            else{
                ind=i;
                val=store[i];
            }
        }
        for (long long i=0;i<n;i++){
            cout<<ans[i]<<" ";
        }

        //T.C.:- O(n)
        //S.C.:- O(n);
        cout<<endl;
    }
    return 0;
}
