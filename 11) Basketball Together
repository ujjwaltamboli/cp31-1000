#include <bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int d;
    cin>>d;
    vector<int> store(n,0);
    for (int i=0;i<n;i++){
        cin>>store[i];
    } //O(n)
    sort(store.rbegin(), store.rend());
    d=d+1;
    int count=0;
    int t=n;
    for (int i=0;i<t;i++){
        n=n-ceil(d/float(store[i]));
        count=count+1;
        if (n<0){
            count=count-1;
            break;
        }
    } //O(n)

    //T.C.:- O(n)
    //S.C.:- O(n)
    cout<<count<<endl;
    return 0;
}
